    permissions: 0644
    owner: root:root
    content: AAAAA-BBBBB-CCCCC-DDDDD-EEEEEEE
  - path: /etc/vadc-init/aliyun-setup
    permissions: 0755
    owner: root:root
    encoding: gz+b64
